<!-- Loop over each object in the projects array style="height: 10rem;"-->



 {{#each posts as |post| }}
<div class="btn btn-primary">
  
    <h2><a href="/post-comment/{{post.id}}">{{post.title}} created by {{post.user.username}} on {{format_date post.date_created}}</a></h2>
  
</div>
{{/each}}
  

